{
  "name": "vue-checkers",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service build && cd server/ && npm install && node server.js",
    "start": "npm install && vue-cli-service build && cd server/ && npm install && node server.js",
    "build": "vue-cli-service build",
    "watch": "vue-cli-service build --watch",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.1",
    "body-parser": "^1.19.0",
    "d3": "^5.15.0",
    "eslint": "^5.16.0",
    "express": "^4.17.1",
    "mongoose": "^5.8.10",
    "socket.io": "^2.4.0",
    "vue": "^2.6.11",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.1.5",
    "vue-socket.io": "^2.1.1",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "nodemon": "^1.19.4",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  }
}
